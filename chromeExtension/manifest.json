{
  "name": "Bias removal PDF",
  "version": "0.1",
  "description": "An extension to remove bias when reading research papers",
  "minimum_chrome_version": "16.0.884",
  "permissions": ["downloads", "<all_urls>", "activeTab", "tabs", "*://*.someUrlMatch.xyz/*/*",  "https://ajax.googleapis.com/"],

  "content_scripts": [
    {
      "run_at" : "document_end",
      "matches": ["https://dl.acm.org/*"],
      "js": ["dlacm.js"]
    },
    {
      "run_at" : "document_end",
      "matches": ["https://ieeexplore.ieee.org/*"],
      "js": ["ieee.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {"default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
